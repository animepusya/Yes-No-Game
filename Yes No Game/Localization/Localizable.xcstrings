    "rules.start.body" : {
      "localizations" : {
        "en" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "The host picks a card and reads its title and description out loud. The host checks the hint and the full story in advance, but does not show them to the other players."
          }
        },
        "ru" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Ведущий выбирает карточку и зачитывает её название и описание. Подсказку и полную историю он смотрит заранее, но другим игрокам их не показывает."
          }
        }
      }
    },
    "rules.start.title" : {
